{
    "manifest_version": 3,
    "name": "Tab Inactivator",
    "version": "0.0.0.1",
    "description": "Manually inactivate tabs to save memory.",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage", "notifications"],
    "background": {
        "service_worker": "background.js"
    }
}