{
    "manifest_version": 3,
    "name": "Chrome Tab Inactivator",
    "version": "1.0",
    "description": "Manage and inactivate browser tabs to save memory",
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "windows",
        "system.memory"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "16x16.png",
            "48": "48x48.png",
            "128": "128x128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "16x16.png",
        "48": "48x48.png",
        "128": "128x128.png"
    }
}