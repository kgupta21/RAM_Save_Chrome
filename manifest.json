{
    "manifest_version": 3,
    "name": "RAM Save Chrome",
    "version": "1.0",
    "description": "Manage and inactivate browser tabs to save memory",
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "windows",
        "system.memory"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icon48.svg",
            "128": "icon128.svg"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon48.svg",
        "128": "icon128.svg"
    }
}